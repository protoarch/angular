{
    "name": "protoarch",
    "version": "0.0.0-development",
    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "nx": "nx",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "npm exec -- stylelint \"**/*.less\" && npm exec -- eslint --config ./.eslintrc.js --debug \"**/*.{js,ts,html}\"",
        "lint:fix": "npm exec -- stylelint --fix \"**/*.less\" && npm exec -- eslint --config ./.eslintrc.js --fix --debug \"**/*.{js,ts,html}\"",
        "lint:scss": "npm exec -- stylelint \"**/*.less\"",
        "lint:scss:fix": "npm exec -- stylelint --fix \"**/*.less\"",
        "e2e": "ng e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
    "workspaces": [
        "apps/*",
        "libs/*"
    ],
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/common": "13.2.0",
        "@angular/compiler": "13.2.0",
        "@angular/core": "13.2.0",
        "@angular/forms": "13.2.0",
        "@angular/platform-browser": "13.2.0",
        "@angular/platform-browser-dynamic": "13.2.0",
        "@angular/router": "13.2.0",
        "@nrwl/angular": "13.6.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.1.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.2.0",
        "@angular-eslint/eslint-plugin": "13.0.1",
        "@angular-eslint/eslint-plugin-template": "13.0.1",
        "@angular-eslint/template-parser": "13.0.1",
        "@angular/cli": "13.2.0",
        "@angular/compiler-cli": "13.2.0",
        "@angular/language-service": "13.2.0",
        "@nrwl/cli": "13.6.1",
        "@nrwl/cypress": "13.6.1",
        "@nrwl/eslint-plugin-nx": "13.6.1",
        "@nrwl/jest": "13.6.1",
        "@nrwl/linter": "13.6.1",
        "@nrwl/nx-cloud": "13.1.2",
        "@nrwl/tao": "13.6.1",
        "@nrwl/workspace": "13.6.1",
        "@types/jest": "27.0.2",
        "@types/node": "14.14.33",
        "@typescript-eslint/eslint-plugin": "~5.20.0",
        "@typescript-eslint/parser": "~5.20.0",
        "conventional-changelog": "^3.1.24",
        "conventional-changelog-cli": "^2.1.1",
        "cpy-cli": "^3.1.1",
        "cypress": "^7.3.0",
        "dotenv": "10.0.0",
        "eslint": "~8.12.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "jest": "27.2.3",
        "jest-preset-angular": "11.0.0",
        "husky": "^4.2.5",
        "lint-staged": "^12.1.2",
        "ng-packagr": "13.1.3",
        "np": "^7.5.0",
        "rimraf": "^2.6.3",
        "postcss": "^8.3.9",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^6.7.0",
        "postcss-url": "^10.1.1",
        "prettier": "2.5.1",
        "ts-jest": "27.0.5",
        "ts-node": "~9.1.1",
        "typescript": "4.5.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/protoarch/angular.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{less,css,scss}": [
            "npx stylelint --fix"
        ],
        "*.{md,json,html}": [
            "npx prettier --write"
        ],
        "*.{js,ts,html}": [
            "npx eslint --fix"
        ]
    }
}
